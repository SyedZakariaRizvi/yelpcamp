<% layout("layouts/boilerplate.ejs") %>
<body>
    <h1>All Campgrounds</h1>
    <div>
        <a href="/campgrounds/new">Add a new Campground</a>
    </div>
    <ul>
        <% for(let campground of campgrounds) { %>
            <a href="/campgrounds/<%= campground._id %>"><li><%= campground.title %></li></a>
        <% } %>
    </ul>
</body>